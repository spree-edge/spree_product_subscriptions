<% content_for :page_title do %>
  <%= link_to Spree.t(:subscription_frequencies), spree.admin_subscription_frequencies_url %> /
  <%= @subscription_frequency.title.capitalize %>
<% end %>

<%= form_for @subscription_frequency, url: object_url, method: :put do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
